// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Cache {
  id Int @id @default(autoincrement())
  key String @unique 
  data Json
  isDirty Boolean
}

model Settings {
  id Int @id @default(autoincrement())
  key String @unique
  value String
}

model MailingList {
  id Int @id @default(autoincrement())
  email String @unique
}